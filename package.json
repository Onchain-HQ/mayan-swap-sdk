{
  "name": "@mayanfinance/swap-sdk",
  "description": "A SDK to swap with Mayan",
  "version": "1.0.4",
  "source": "src/main.ts",
  "main": "dist/main.umd.js",
  "module": "dist/main.modern.module.js",
  "unpkg": "dist/main.umd.js",
  "types": "dist/main.d.ts",
  "keywords": [
    "mayan",
    "mayanfinance",
    "mayansdk",
    "swap-sdk"
  ],
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ],
  "scripts": {
    "build": "microbundle --define PKG_VERSION=$npm_package_version",
    "watch": "microbundle watch --define PKG_VERSION=$npm_package_version",
    "lint": "prettier src -c",
    "format": "prettier src --write",
    "prepack": "rm -rf dist && npm run build",
    "release": "npm run prepack && npx np --no-tests"
  },
  "devDependencies": {
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "author": "Jiri",
  "dependencies": {
    "@solana/buffer-layout": "^4.0.0",
    "@solana/web3.js": "^1.66.0",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.7.2",
    "js-sha3": "^0.8.0"
  }
}
